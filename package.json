{
  "name": "ezviz-rag-agent",
  "version": "1.0.0",
  "description": "RAG-powered API for EZVIZ SDK error code lookups and troubleshooting",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "docker:dev": "docker compose up chromadb api-dev",
    "docker:watch": "docker compose watch",
    "docker:prod": "docker compose --profile prod up api"
  },
  "keywords": [
    "rag",
    "ezviz",
    "chromadb",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "chromadb": "^1.8.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}